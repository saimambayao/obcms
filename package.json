{
  "name": "obcms",
  "version": "1.0.0",
  "description": "Office for Other Bangsamoro Communities Management System",
  "scripts": {
    "build:css": "NODE_ENV=production postcss ./src/static/css/input.css -o ./src/static/css/output.css",
    "build:css:dev": "postcss ./src/static/css/input.css -o ./src/static/css/output.css",
    "watch:css": "postcss ./src/static/css/input.css -o ./src/static/css/output.css --watch",
    "dev": "npm run watch:css"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.13",
    "@playwright/test": "^1.55.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "cssnano": "^7.1.1",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.0",
    "tailwindcss": "^3.4.17"
  }
}
